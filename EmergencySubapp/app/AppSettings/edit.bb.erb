<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @appsettings.object %>
    <h1>Edit <%= @appsettings.TimeIncrement%></h1>
    <%= link_to "Delete", :action => :delete, :id => @appsettings.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @appsettings.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Timeincrement: </td>
              <td class="itemValue"><input type="text" name="appsettings[TimeIncrement]" value="<%= @appsettings.TimeIncrement %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Notificationtypes: </td>
              <td class="itemValue"><input type="text" name="appsettings[NotificationTypes]" value="<%= @appsettings.NotificationTypes %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
