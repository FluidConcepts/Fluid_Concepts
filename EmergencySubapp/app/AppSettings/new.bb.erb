<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New AppSettings</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @appsettings.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Timeincrement: </td>
                <td class="itemValue"><input type="text" name="appsettings[TimeIncrement]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Notificationtypes: </td>
                <td class="itemValue"><input type="text" name="appsettings[NotificationTypes]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
